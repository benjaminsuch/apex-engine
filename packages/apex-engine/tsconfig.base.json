{
  "compilerOptions": {
    "declaration": false,
    "experimentalDecorators": true,
    "isolatedModules": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "removeComments": false,
    "skipLibCheck": true,
    "strict": true,
    "strictFunctionTypes": true,
    "sourceMap": false,
    "target": "ESNext",
  }
}
